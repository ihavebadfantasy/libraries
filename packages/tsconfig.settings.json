{
  "compilerOptions": {
    "module": "esnext",
    "target": "es5",
    "moduleResolution": "node",
    "jsx": "react",
    "lib": ["dom", "dom.iterable", "es6"],
    "isolatedModules": false,
    "noEmitOnError": false,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "noEmitHelpers": true,
    "allowUnreachableCode": true
  }
}
